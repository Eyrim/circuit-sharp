{
  "executable": "CircuitSharp",
  "splashscreen": {
    "imageFile": "../../../wwwroot/GenericData/Imgs/splashScreen.png"
  },
  "name": "CircuitSharp",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.CircuitSharp.app",
    "productName": "CircuitSharp",
    "copyright": "Copyright Â© 2020",
    "buildVersion": "0.1.2",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}